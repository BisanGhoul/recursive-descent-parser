<Pane prefHeight="592.0" prefWidth="817.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller">
   <children>
      <Button layoutX="62.0" layoutY="70.0" mnemonicParsing="false" onAction="#browse" text="Choose File" />
      <TableView fx:id="tbv" layoutX="255.0" layoutY="26.0" prefHeight="334.0" prefWidth="548.0">
         <columns>
            <TableColumn fx:id="cl1" prefWidth="96.0" text="ID" />
            <TableColumn fx:id="cl3" prefWidth="140.0" text="Type" />
            <TableColumn fx:id="cl2" prefWidth="311.0" text="Token" />
